{
  "name": "react-styleguide-enterprise",
  "description": "React components style guide generator",
  "version": "0.5.0",
  "author": "Marvin Hagemeister <marvin@marvinhagemeister.de>",
  "repository": {
    "type": "git",
    "url": "git://github.com/marvinhagemeister/react-styleguidist.git"
  },
  "bugs": {
    "url": "https://github.com/marvinhagemeister/react-styleguidist/issues"
  },
  "license": "MIT",
  "main": "scripts/index.js",
  "bin": {
    "styleguidist": "bin/styleguidist.js"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-runtime": "6.11.6",
    "babel-standalone": "6.17.0",
    "chalk": "1.1.3",
    "entities": "1.1.1",
    "express": "4.14.0",
    "findup": "0.1.5",
    "glob": "7.1.1",
    "highlight.js": "9.7.0",
    "json-loader": "0.5.4",
    "loader-utils": "^0.2.16",
    "lodash": "4.16.4",
    "minimist": "1.2.0",
    "prettyjson": "1.1.3",
    "react-docgen": "2.11.0",
    "remark": "~6.0.1",
    "remark-parse": "~2.0.2",
    "semver-utils": "1.1.1",
    "unified": "~5.1.0",
    "unist-util-visit": "~1.1.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-merge": "0.14.1"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0",
    "webpack": "1 || ^2.1.0-beta"
  },
  "devDependencies": {
    "babel-eslint": "~7.0.0",
    "babel-register": "~6.16.3",
    "chai": "^3.5.0",
    "cross-env": "~3.1.3",
    "css-modules-require-hook": "~4.0.5",
    "dog-names": "~1.0.2",
    "enzyme": "~2.5.1",
    "eslint": "~3.8.1",
    "eslint-config-tamia": "~3.1.1",
    "eslint-plugin-react": "~6.4.1",
    "expect-react-shallow": "~1.2.1",
    "gh-pages": "~0.11.0",
    "mocha": "^3.1.2",
    "mock-require": "~1.3.0",
    "react": "~15.3.2",
    "react-addons-test-utils": "~15.3.2",
    "react-dom": "~15.3.2",
    "react-modal": "~1.5.2",
    "sinon": "~1.17.6",
    "unexpected": "~10.18.1",
    "unexpected-react": "~3.2.4",
    "webpack": "1.13.2 || ^2.1.0-beta"
  },
  "scripts": {
    "test": "npm run lint && npm run test:mocha",
    "test:mocha": "cross-env NODE_ENV=test mocha --compilers js:babel-register 'src/**/*.spec.js' 'test/**/*.spec.js'",
    "test:watch": "npm run test:mocha -- --watch",
    "start": "bin/styleguidist.js server --config examples/basic/styleguide.config.js",
    "start:customised": "bin/styleguidist.js server --config examples/customised/styleguide.config.js",
    "start:sections": "bin/styleguidist.js server --config examples/sections/styleguide.config.js",
    "lint": "eslint . --ext .js",
    "build": "bin/styleguidist.js build --config examples/basic/styleguide.config.js",
    "build:customised": "bin/styleguidist.js build --config examples/customised/styleguide.config.js",
    "build:sections": "bin/styleguidist.js build --config examples/sections/styleguide.config.js",
    "prepublish": "npm t"
  }
}
